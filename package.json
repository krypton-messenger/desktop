{
  "name": "krypton_desktop",
  "version": "1.0.0",
  "description": "Krypton Messenger for Desktop",
  "repository": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "reset": "node reset",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . krypton-desktop --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.ProductName=\"Krypton Desktop\"",
    "package-linux": "electron-packager . krypton-desktop --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "keywords": [],
  "author": "Tim T.",
  "license": "AGPL-3.0",
  "devDependencies": {
    "electron": "^12.0.1",
    "electron-packager": "^15.2.0"
  },
  "dependencies": {
    "exec-sh": "^0.4.0",
    "fs": "0.0.1-security",
    "got": "^11.8.2",
    "material-components-web": "^10.0.0",
    "nconf": "^0.11.2",
    "node-forge": "^0.10.0",
    "open": "^8.2.1",
    "path": "^0.12.7",
    "ws": "^7.5.0"
  }
}
